{
  "name": "backend",
  "version": "1.0.0",
  "main": "azure-proxy.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/server.js",
    "prisma:migrate": "prisma migrate dev --name init",
    "studio:kill": "powershell -NoProfile -Command \"& { $n=(netstat -ano | findstr :5555); if($n){ $pid=($n -split ' +')[-1]; if($pid -match '^\\d+$'){ taskkill /PID $pid /F } } }\"",
    "prisma:studio": "npm run studio:kill && prisma studio --schema prisma/schema.prisma --port 5555",
    "prisma:seed": "node --env-file=.env prisma/seed.js",
    "health": "node scripts/health-check.js",
    "health:verbose": "node scripts/health-check.js --verbose",
    "health:json": "node scripts/health-check.js --json",
    "health:save": "node scripts/health-check.js --json --verbose",
    "health:schedule": "node scripts/scheduled-health-check.js",
    "health:monitor": "npm run health:schedule"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@azure/ai-form-recognizer": "^4.0.0",
    "@prisma/client": "6.14.0",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "bcrypt": "6.0.0",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "8.0.1",
    "file-type": "^21.0.0",
    "helmet": "8.1.0",
    "hpp": "0.2.3",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "9.0.2",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "opossum": "^9.0.0",
    "otplib": "12.0.1",
    "pino": "9.9.0",
    "pino-http": "10.5.0",
    "pino-pretty": "13.1.1",
    "prisma": "6.14.0",
    "prom-client": "^15.1.3",
    "qrcode": "1.5.4",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "uuid": "11.1.0",
    "yamljs": "0.3.0",
    "zod": "4.1.1"
  }
